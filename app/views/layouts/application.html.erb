<!DOCTYPE html>
<html>
<head>
  <title>Mypetsns</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>


<!-- <div class="all"> -->
<!-- <div class="wrapper" > -->
  <%= link_to root_path do %>
    <%= image_tag '/images/hello human-logo.png',class:'applogo'%>
  <% end %>  

	
	<header>

		<ul>
		  <% unless user_signed_in? %>
			<li><%= link_to '新規登録',new_user_registration_path,class:'backclear' %></li>
			<li><%= link_to 'login',new_user_session_path,class:'backclear' %></li>
			<li><%= link_to 'facebook',user_omniauth_authorize_path(:facebook),class:'backclear' %></li>
			<li><%= link_to 'twitter',user_omniauth_authorize_path(:twitter),class:'backclear'%></li>
          <% else %>
			<li><%= link_to 'logout',destroy_user_session_path ,method: :delete ,class:'backclear'%></li>
			<li><%= link_to 'ハローする',new_note_path ,class:'backclear'%></li>
			<li><%= link_to 'マイページ',usershow_path(current_user.id),class:'backclear' %></li>
			<li><%= link_to 'Q&A',questions_path,class:'backclear' %></li>
			<li><%= link_to 'ハロー一覧',notes_path ,class:'backclear'%></li>
		  <% end %>
			
		</ul>
	</header>
	<!-- <h1>PETS</h1>
	 -->

	<div class="appcontent">
		<%= yield %>
	</div>
<!-- </div>
	<div class="content">
	</div>
</div> -->

</body>
</html>
