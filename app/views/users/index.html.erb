<div class="pet_form">
 <h2><%= @title %></h2>

  <% @users.each do |user| %>
    <p class="user_index_border"> <% if user.image? %>
      <%= image_tag user.image.thumb.url,class:'user_index_images img-thumbnail' %>
    <% else %>
      <%= image_tag '/images/logo (2).png',class:'user_index_images img-thumbnail'%>
    <% end %><%= link_to user.name,usershow_path(user.id) %></p>
  <% end %>
</div>